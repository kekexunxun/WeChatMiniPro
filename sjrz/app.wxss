/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.baoming-btn-small {
  display: inline-block;
  font-size: 22rpx;
  border: 1rpx solid red;
  border-radius: 20rpx;
  background-color: white;
  color: red;
  line-height: 40rpx;
  padding: 0 6px;
  margin-right: 20rpx;
}

.baoming-btn-larger {
  padding: 0 6px;
  margin-right: 20rpx;
  font-size: 13px;
  display: inline-block;
  line-height: 25px;
  border: 1rpx solid red;
  border-radius: 10px;
  background-color: white;
  color: red;
}

